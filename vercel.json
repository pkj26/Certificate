{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap.xml"
    },
    {
      "source": "/robots.txt",
      "destination": "/robots.txt"
    },
    {
      "source": "/ads.txt",
      "destination": "/ads.txt"
    },
    {
      "source": "/BingSiteAuth.xml",
      "destination": "/BingSiteAuth.xml"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}